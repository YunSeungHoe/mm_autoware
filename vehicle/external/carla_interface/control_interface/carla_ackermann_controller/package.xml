<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
  <name>carla_ackermann_controller</name>
  <version>1.0.0</version>
  <description>The carla_ackermann_controller package</description>
  <license>Apache License 2.0</license>

  <maintainer email="mjiw9440@naver.com">Jiwon Moon</maintainer>

  <buildtool_depend>ament_cmake_auto</buildtool_depend>

  <build_depend>autoware_cmake</build_depend>

  <depend>rclcpp</depend>
  <depend>rclcpp_components</depend>
  <depend>carla_msgs</depend>
  <depend>carla_ackermann_msgs</depend>
  <depend>autoware_auto_control_msgs</depend>
  <depend>tf2</depend>
  <depend>tf2_geometry_msgs</depend>
