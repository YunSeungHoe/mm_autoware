<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
  <name>carla_vehicle_status_reporter</name>
  <version>1.0.0</version>
  <description>Packages for CARLA vehicle status report</description>

  <maintainer email="mjiw9440@naver.com">Jiwon Moon</maintainer>

  <license>Apache License 2.0</license>

  <author email="mjiw9440@naver.com">Jiwon Moon</author>

  <buildtool_depend>ament_cmake_auto</buildtool_depend>

  <build_depend>autoware_cmake</build_depend>

  <depend>autoware_auto_vehicle_msgs</depend>
  <depend>carla_msgs</depend>
  <depend>rclcpp</depend>
  <depend>rclcpp_components</depend>

  <exec_depend>ros2launch</exec_depend>

  <export>
    <build_type>ament_cmake</build_type>
  </export>
</package>
